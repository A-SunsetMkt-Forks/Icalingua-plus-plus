        "copy-packages": "mkdir ./build/node_modules && mkdir ./build/node_modules/@icalingua && cp -a ../packages/storageProviders/build ./build/node_modules/@icalingua/storage-providers && cp -a ../packages/types/build ./build/node_modules/@icalingua/types && cp -a ./build/node_modules/@icalingua ./build/@icalingua",
        "copy-packages:win": "mkdir .\\build\\node_modules && mkdir .\\build\\node_modules\\@icalingua && xcopy /e ..\\packages\\storageProviders\\build\\ .\\build\\node_modules\\@icalingua\\storage-providers\\ && xcopy /e ..\\packages\\types\\build\\ .\\build\\node_modules\\@icalingua\\types\\ && xcopy /e .\\build\\node_modules\\@icalingua\\ .\\build\\@icalingua\\",
        "detach-start": "screen -U pnpm start",
        "dev": "NODE_ENV=development ts-node ."
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
